# Nord
# https://www.nordtheme.com/docs/colors-and-palettes

# Polar night, background
set nord00 2E3440 # background dark
set nord01 3B4252 # status bar color, borders
set nord02 434C5E # current text editor line
set nord03 4C566A # comments, etc.
# Snow storm 
set nord04 D8DEE9 # variables, constants, attributes, fields
set nord05 E5E9F0 # subtle highlighted text
set nord06 ECEFF4 # text color
# Frost 
set nord07 8FBCBB # stand out UI, classes, types, primitives
set nord08 88C0D0 # most visual attention, declarations, functions
set nord09 81A1C1 # secondary UI elements, operators
set nord10 5E81AC # tertiary UI elements, pragmas, pre-processors
# Aurora
set nord11 BF616A # rouge
set nord12 D08770 # orange
set nord13 EBCB8B # jaune
set nord14 A3BE8C # vert
set nord15 B48EAD # violet


#set bg_dark 282C34
set bg_dark $nord00
set bg_light $nord02
set fg_dark $nord03
#set fg_normal abb2bf
set fg_normal $nord04
set fg_light $nord05
set light_white $nord06
set red $nord11
set green $nord14
set yellow $nord13
set blue $nord09
set magenta $nord15
set cyan $nord08
set light_red $nord11
set light_green $nord14
set light_yellow $nord12
set light_blue $nord09
set light_magenta $nord15
set light_cyan $nord07
