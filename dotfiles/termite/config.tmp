# Font
[options]
font = Liberation Mono 13
#font = JetBrains Mono 13
#cell_height_scale = 1.2
allow_bold = false
bold_is_bright = true
mouse_autohide = true

[colors]
#base03 base0
background = #@bg_dark
foreground = #@fg_normal
foreground_bold = #@fg_light
highlight = #$bg_lighter
cursor = #$fg_normal

# Black, Gray
# base02 base01
color0 = #@bg_light
color8 = #@fg_dark

# Red (31)
color1 = #@red
color9 = #@light_red

# Green (32)
color2 = #@green
color10 = #@light_green

# Yellow (33)
color3 = #@yellow
color11 = #@light_yellow

# Blue (34)
color4 = #@blue
color12 = #@light_blue

# magenta (35)
color5 = #@magenta
color13 = #@light_magenta

# cyan (36)
color6 = #@cyan
color14 = #@light_cyan

# Silver white (37)
color7 = #@fg_dark
color15 = #@fg_light
